<UserControl x:Class="WGPM.R.UI.UIVehicals.Xjc.W_Xjc2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WGPM.R.UI.UIVehicals.Xjc"
             mc:Ignorable="d" 
             d:DesignHeight="56" d:DesignWidth="98">
    <Grid>
        <local:HeadStock x:Name="headStock" Margin="49,0,0,0"/>
        <TextBox Margin="0,0,49,0" x:Name="can" Height="49" Width="49" BorderBrush="Blue" >
            <TextBox.Background>
                <RadialGradientBrush>
                    <GradientStop Color="Red" Offset="0" />
                    <GradientStop Color="Black" Offset="1" />
                </RadialGradientBrush>
            </TextBox.Background>
        </TextBox>
        <local:CokeCan Margin="98,0,-49,0" Visibility="Collapsed"/>
    </Grid>
</UserControl>
